<%= render 'shared/flash_messages' %>
<div class="container border p-3">
  <h1>トップページ</h1>
  <%= form_with model: @record, local: true do |form| %>
    <% @members.each do |member| %>
      <p>
        <%= check_box_tag "name[]", member.id, false, id: "name_#{member.id}" %>
        <%= label_tag "name_#{member.id}", member.name %>
      </p>
    <% end %>
    <%= form.submit "送信" %>
  <% end %>
</div>
